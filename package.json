{
  "name": "remark-heading-gap",
  "version": "1.0.0",
  "description": "Adjust the gap between headings.",
  "main": "dist/index.js",
  "files": [
    "LICENSE-MIT",
    "dist"
  ],
  "scripts": {
    "pretest": "eslint src",
    "markdown": "remark *.md --quiet",
    "prepublish": "del-cli dist && BABEL_ENV=publish babel src --out-dir dist --ignore /__tests__/",
    "test": "npm run markdown && nyc --babel --reporter=text ava src/__tests__"
  },
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "coveralls": "^2.11.6",
    "del-cli": "^0.2.0",
    "eslint": "^1.10.3",
    "nyc": "^5.5.0",
    "remark": "^3.2.2",
    "remark-usage": "^2.1.0"
  },
  "keywords": [
    "heading",
    "gap",
    "space",
    "newlines",
    "whitespace",
    "remark",
    "markdown"
  ],
  "license": "MIT",
  "homepage": "https://github.com/ben-eb/remark-heading-gap",
  "author": {
    "name": "Ben Briggs",
    "email": "beneb.info@gmail.com",
    "url": "http://beneb.info"
  },
  "repository": "ben-eb/remark-heading-gap",
  "remarkConfig": {
    "output": true,
    "plugins": {
      "dist/index.js": {},
      "usage": {
        "heading": "example"
      }
    },
    "settings": {
      "bullet": "*",
      "fences": true,
      "listItemIndent": "1"
    }
  }
}
